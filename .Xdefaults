! TAMZEN
URxvt.font:              -*-terminus-medium-*-*-*-16*-*-*-*-*-*-*
! URxvt.font:            -misc-tamzenforpowerline-*-r-normal-*-17-145-100-100-*-100-iso10646-1
! URxvt.boldFont:        -misc-tamzenforpowerline-*-r-normal-*-17-145-100-100-*-100-iso10646-1
! URxvt.italicFont:      -misc-tamzen-medium-r-*--20-*-*-*-*-*-*-*
! URxvt.boldItalicFont:  -misc-tamzen-bold-r-*--20-*-*-*-*-*-*-*

urxvt*scrollBar: false
urxvt*internalBorder: 8
urxvt*transparent: true
urxvt*shading: 10

urxvt*confirmExit: true
urxvt*confirmClose: true

urxvt*urlLauncher: chromium

URxvt.keysym.C-C: perl:clipboard:copy
URxvt.keysym.C-V: perl:clipboard:paste
URxvt.keysym.C-A-V: perl:clipboard:paste_escaped

URxvt.perl-ext-common:  default,clipboard,matcher
URxvt.iso14755 : false

URxvt.transparent: true
URxvt.fading: 10
